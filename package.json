{
  "name": "freshlytics",
  "version": "1.0.0-beta",
  "repository": "https://github.com/sheshbabu/freshlytics",
  "description": "Analytics",
  "author": "Sheshbabu Chinnakonda",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist",
    "dev": "npm run dev:server & npm run dev:client",
    "build": "rm -rf ./dist && npm run build:server && npm run build:client",
    "dev:server": "nodemon -q -e ts -w ./src/server -x ts-node ./src/server/index.ts",
    "dev:client": "webpack --mode=development -w --config ./src/client/webpack.config.js",
    "build:server": "tsc --build ./src/server/tsconfig.server.json",
    "build:client": "webpack --mode=production --progress --profile --config ./src/client/webpack.config.js",
    "migrate": "node-pg-migrate -m ./src/server/migrations -j sql up",
    "migrate:create": "node-pg-migrate -m ./src/server/migrations -j sql create",
    "seed": "ts-node ./src/server/seeds",
    "seed:test": "ts-node ./tests/fixtures/PageViews.fixture.ts",
    "lint": "eslint --ext .ts,.tsx .",
    "test:load": "artillery run ./tests/load/artillery.yml",
    "precommit": "lint-staged"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-pg-simple": "6.0.1",
    "cors": "2.8.5",
    "date-fns": "1.30.1",
    "express": "4.17.0",
    "express-session": "1.16.2",
    "frappe-charts": "1.1.0",
    "luxon": "^1.17.2",
    "nanoid": "^2.0.3",
    "node-pg-migrate": "5.7.0",
    "pg": "7.11.0",
    "react": "16.8.6",
    "react-clipboard.js": "^2.0.13",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "semantic-ui-calendar-react": "0.15.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.86.0",
    "ts-node": "8.8.2",
    "typescript": "3.8.3",
    "useragent": "2.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/compression": "0.0.36",
    "@types/connect-pg-simple": "4.2.0",
    "@types/cors": "2.8.5",
    "@types/date-fns": "2.6.0",
    "@types/express": "4.16.1",
    "@types/express-session": "1.15.12",
    "@types/luxon": "^1.15.2",
    "@types/nanoid": "^2.0.0",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.4",
    "@types/useragent": "2.1.1",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "artillery": "1.6.0-28",
    "awesome-typescript-loader": "5.2.1",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "mini-css-extract-plugin": "0.6.0",
    "nodemon": "1.19.0",
    "prettier": "1.17.1",
    "source-map-loader": "0.2.4",
    "url-loader": "1.1.2",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
